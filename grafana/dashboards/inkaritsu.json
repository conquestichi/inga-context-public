{
  "uid": "inkaritsu-main",
  "title": "Inkaritsu モニタ",
  "schemaVersion": 39,
  "version": 6,
  "editable": true,
  "refresh": "10s",
  "time": {
    "from": "now-6h",
    "to": "now"
  },
  "timepicker": {
    "refresh_intervals": [
      "5s",
      "10s",
      "30s",
      "1m",
      "5m",
      "15m",
      "30m",
      "1h"
    ]
  },
  "links": [
    {
      "type": "link",
      "title": "Prometheus",
      "url": "http://localhost:39090",
      "targetBlank": true
    },
    {
      "type": "link",
      "title": "Alertmanager",
      "url": "http://localhost:39093",
      "targetBlank": true
    }
  ],
  "panels": [
    {
      "type": "timeseries",
      "title": "手動RPMの推移（目標60）",
      "gridPos": {
        "x": 0,
        "y": 0,
        "w": 16,
        "h": 8
      },
      "datasource": {
        "type": "prometheus",
        "uid": "inkaritsu-prom"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "rpm",
          "min": 0,
          "max": 80
        },
        "overrides": []
      },
      "targets": [
        {
          "refId": "A",
          "expr": "inkaritsu_manual_rpm",
          "editorMode": "code"
        }
      ]
    },
    {
      "type": "gauge",
      "title": "現在の手動RPM",
      "gridPos": {
        "x": 16,
        "y": 0,
        "w": 8,
        "h": 8
      },
      "datasource": {
        "type": "prometheus",
        "uid": "inkaritsu-prom"
      },
      "options": {
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ],
          "fields": "",
          "values": false
        },
        "showThresholdLabels": true,
        "showThresholdMarkers": true
      },
      "fieldConfig": {
        "defaults": {
          "unit": "rpm",
          "min": 0,
          "max": 80,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "red",
                "value": 0
              },
              {
                "color": "yellow",
                "value": 59
              },
              {
                "color": "green",
                "value": 60
              }
            ]
          }
        },
        "overrides": []
      },
      "targets": [
        {
          "refId": "A",
          "expr": "inkaritsu_manual_rpm",
          "instant": true,
          "editorMode": "code"
        }
      ]
    },
    {
      "type": "stat",
      "title": "稼働時間（分）",
      "gridPos": {
        "x": 0,
        "y": 8,
        "w": 8,
        "h": 6
      },
      "datasource": {
        "type": "prometheus",
        "uid": "inkaritsu-prom"
      },
      "options": {
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ],
          "fields": "",
          "values": false
        },
        "orientation": "horizontal"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "m",
          "decimals": 1
        },
        "overrides": []
      },
      "targets": [
        {
          "refId": "A",
          "expr": "inkaritsu_uptime_seconds/60",
          "instant": true,
          "editorMode": "code"
        }
      ]
    },
    {
      "type": "stat",
      "title": "RPM乖離（60基準）",
      "gridPos": {
        "x": 8,
        "y": 8,
        "w": 8,
        "h": 6
      },
      "datasource": {
        "type": "prometheus",
        "uid": "inkaritsu-prom"
      },
      "options": {
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ],
          "fields": "",
          "values": false
        },
        "orientation": "horizontal"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "rpm",
          "decimals": 1,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": 0
              },
              {
                "color": "yellow",
                "value": 1
              },
              {
                "color": "red",
                "value": 5
              }
            ]
          }
        },
        "overrides": []
      },
      "targets": [
        {
          "refId": "A",
          "expr": "abs(inkaritsu_manual_rpm - 60)",
          "instant": true,
          "editorMode": "code"
        }
      ]
    },
    {
      "type": "stat",
      "title": "Webhook状態",
      "gridPos": {
        "x": 16,
        "y": 8,
        "w": 8,
        "h": 6
      },
      "datasource": {
        "type": "prometheus",
        "uid": "inkaritsu-prom"
      },
      "options": {
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ],
          "fields": "",
          "values": false
        },
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "auto",
        "orientation": "horizontal"
      },
      "fieldConfig": {
        "defaults": {
          "mappings": [
            {
              "type": "value",
              "options": {
                "0": {
                  "text": "DOWN"
                },
                "1": {
                  "text": "UP"
                }
              }
            }
          ],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "red",
                "value": 0
              },
              {
                "color": "green",
                "value": 1
              }
            ]
          }
        },
        "overrides": []
      },
      "targets": [
        {
          "refId": "A",
          "expr": "up{job=\"inkaritsu_webhook\"}",
          "instant": true,
          "editorMode": "code"
        }
      ]
    },
    {
      "type": "stat",
      "title": "スクレイプ遅延（5分平均, ms）",
      "gridPos": {
        "x": 0,
        "y": 14,
        "w": 8,
        "h": 6
      },
      "datasource": {
        "type": "prometheus",
        "uid": "inkaritsu-prom"
      },
      "options": {
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ],
          "fields": "",
          "values": false
        },
        "orientation": "horizontal"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "decimals": 0,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": 0
              },
              {
                "color": "yellow",
                "value": 500
              },
              {
                "color": "red",
                "value": 2000
              }
            ]
          }
        },
        "overrides": []
      },
      "targets": [
        {
          "refId": "A",
          "expr": "avg_over_time(scrape_duration_seconds{job=\"inkaritsu_webhook\"}[5m]) * 1000",
          "instant": true,
          "editorMode": "code"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "RPM乖離の推移（|rpm-60|）",
      "gridPos": {
        "x": 8,
        "y": 14,
        "w": 16,
        "h": 6
      },
      "datasource": {
        "type": "prometheus",
        "uid": "inkaritsu-prom"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "rpm",
          "min": 0,
          "max": 20
        },
        "overrides": []
      },
      "targets": [
        {
          "refId": "A",
          "expr": "abs(inkaritsu_manual_rpm - 60)",
          "editorMode": "code"
        }
      ]
    },
    {
      "type": "table",
      "title": "アラート一覧（発火中）",
      "gridPos": {
        "x": 0,
        "y": 20,
        "w": 24,
        "h": 10
      },
      "datasource": {
        "type": "prometheus",
        "uid": "inkaritsu-prom"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "ALERTS{alertstate=\"firing\"}",
          "instant": true,
          "editorMode": "code"
        }
      ],
      "transformations": [
        {
          "id": "labelsToFields",
          "options": {
            "mode": "columns"
          }
        }
      ],
      "options": {
        "showHeader": true
      }
    },
    {
      "type": "timeseries",
      "title": "エクイティカーブ",
      "gridPos": {
        "x": 0,
        "y": 30,
        "w": 24,
        "h": 8
      },
      "datasource": {
        "type": "prometheus",
        "uid": "inkaritsu-prom"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "currencyJPY"
        },
        "overrides": []
      },
      "targets": [
        {
          "refId": "A",
          "expr": "inkaritsu_trading_equity",
          "editorMode": "code"
        }
      ]
    },
    {
      "type": "stat",
      "title": "本日損益（円）",
      "gridPos": {
        "x": 0,
        "y": 38,
        "w": 8,
        "h": 6
      },
      "datasource": {
        "type": "prometheus",
        "uid": "inkaritsu-prom"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "currencyJPY",
          "decimals": 0,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "red",
                "value": -1000000000.0
              },
              {
                "color": "yellow",
                "value": 0
              },
              {
                "color": "green",
                "value": 1
              }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ],
          "fields": "",
          "values": false
        },
        "orientation": "horizontal"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "inkaritsu_trading_today_pnl",
          "instant": true,
          "editorMode": "code"
        }
      ]
    },
    {
      "type": "stat",
      "title": "勝率（%）",
      "gridPos": {
        "x": 8,
        "y": 38,
        "w": 8,
        "h": 6
      },
      "datasource": {
        "type": "prometheus",
        "uid": "inkaritsu-prom"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "decimals": 1,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "red",
                "value": 0
              },
              {
                "color": "yellow",
                "value": 50
              },
              {
                "color": "green",
                "value": 60
              }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ],
          "fields": "",
          "values": false
        },
        "orientation": "horizontal"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "inkaritsu_win_rate_pct/100",
          "instant": true,
          "editorMode": "code"
        }
      ]
    },
    {
      "type": "stat",
      "title": "最大ドローダウン（%）",
      "gridPos": {
        "x": 16,
        "y": 38,
        "w": 8,
        "h": 6
      },
      "datasource": {
        "type": "prometheus",
        "uid": "inkaritsu-prom"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "decimals": 1,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": 0
              },
              {
                "color": "yellow",
                "value": 5
              },
              {
                "color": "red",
                "value": 10
              }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ],
          "fields": "",
          "values": false
        },
        "orientation": "horizontal"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "inkaritsu_max_drawdown_pct/100",
          "instant": true,
          "editorMode": "code"
        }
      ]
    }
  ],
  "templating": {
    "list": []
  }
}